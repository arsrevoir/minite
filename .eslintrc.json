{
  "env": {
      "browser": true,
      "es2022": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
      "accessor-pairs": "warn",
      "block-scoped-var": "error",
      "eqeqeq": "warn",
      "func-style": "warn",
      "semi": ["warn", "never"],
      "no-unused-vars": "warn",
      "no-unused-expressions": "warn",
      "no-undef": "warn"
  }
}